{
  "multipart": [
    {
      "when": {
        "signal": "off"
      },
      "apply": {
        "model": "elemental:block/conduit_core_off"
      }
    },
    {
      "when": {
        "signal": "cooldown"
      },
      "apply": {
        "model": "elemental:block/conduit_core_cooldown"
      }
    },
    {
      "when": {
        "OR": [
          {
            "signal": "on1"
          },
          {
            "signal": "on2"
          }
        ]
      },
      "apply": {
        "model": "elemental:block/conduit_core_on"
      }
    },
    {
      "when": {
        "axis_x": "true",
        "signal": "off"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_off",
        "y": 90
      }
    },
    {
      "when": {
        "axis_x": "true",
        "signal": "cooldown"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_cooldown",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "axis_x": "true",
            "signal": "on1"
          },
          {
            "axis_x": "true",
            "signal": "on2"
          }
        ]
      },
      "apply": {
        "model": "elemental:block/conduit_axis_on",
        "y": 90
      }
    },
    {
      "when": {
        "axis_y": "true",
        "signal": "off"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_off",
        "x": 90
      }
    },
    {
      "when": {
        "axis_y": "true",
        "signal": "cooldown"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_cooldown",
        "x": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "axis_y": "true",
            "signal": "on1"
          },
          {
            "axis_y": "true",
            "signal": "on2"
          }
        ]
      },
      "apply": {
        "model": "elemental:block/conduit_axis_on",
        "x": 90
      }
    },
    {
      "when": {
        "axis_z": "true",
        "signal": "off"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_off"
      }
    },
    {
      "when": {
        "axis_z": "true",
        "signal": "cooldown"
      },
      "apply": {
        "model": "elemental:block/conduit_axis_cooldown"
      }
    },
    {
      "when": {
        "OR": [
          {
            "axis_z": "true",
            "signal": "on1"
          },
          {
            "axis_z": "true",
            "signal": "on2"
          }
        ]
      },
      "apply": {
        "model": "elemental:block/conduit_axis_on"
      }
    }
  ]
}